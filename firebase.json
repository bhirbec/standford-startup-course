{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "app/static",
    "rewrites": [
      {"source": "/", "function": "app"},
      {"source": "/me", "function": "app"},
      {"source": "/in/*", "function": "app"},
      {"source": "/me/experience", "function": "app"},
      {"source": "/me/experience/*", "function": "app"},
      {"source": "/login", "function": "app"},
      {"source": "/signin", "function": "app"}
    ]
  }
}
