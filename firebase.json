{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "app/static",
    "rewrites": [
      {"source": "/", "function": "app"},
      {"source": "/me", "function": "app"},
      {"source": "/me/edit", "function": "app"},
      {"source": "/in/*", "function": "app"},
      {"source": "/me/experience", "function": "app"},
      {"source": "/me/experience/*", "function": "app"},
      {"source": "/onboard", "function": "app"},
      {"source": "/login", "function": "app"},
      {"source": "/signup", "function": "app"},
      {"source": "/logged-in", "function": "app"},
      {"source": "/password-reset", "function": "app"},
      {"source": "/search", "function": "app"},
      {"source": "/test", "function": "app"},
      {"source": "/account/*", "function": "app"},
      {"source": "/message/*", "function": "app"}
    ]
  }
}
